              "title": "Le type etat (parametrage)",
              "default": "",
              "examples": [
                "HS"
              ],
              "pattern": "^(.*)$"
            },
            "Ordre": {
              "$id": "#/items/properties/Pieces/items/properties/Ordre",
              "type": "integer",
              "title": "Ordre",
              "default": 0,
              "examples": [
                10
              ]
            }
